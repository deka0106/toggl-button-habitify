{
  "manifest_version": 2,
  "name": "Toggl Button on Habitify",
  "version": "0.0.1",
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://app.habitify.me/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "storage", "https://api.track.toggl.com/*"]
}
