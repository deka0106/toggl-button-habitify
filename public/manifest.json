{
  "manifest_version": 2,
  "name": "Toggl Button on Habitify",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://app.habitify.me/"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}
